&pinctrl {
    pinctrl-names = "default";
    pinctrl-0 = <&secod23_gpioset>;

	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PA7 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		soc_slppin_gpio: soc_slppin_gpio {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_GPIO &pcfg_output_low>;
		};

		soc_slppin_slp: soc_slppin_slp {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_1 &pcfg_pull_none>;
		};

		soc_slppin_rst: soc_slppin_rst {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_2 &pcfg_pull_none>;
		};
	};

  	eth_phy {
		eth_phy_pwr: eth_phy_pwr {
                        rockchip,pins =
                                <2 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>;
                };
        };

	sdmmc_pwr  {
                sdmmc_pwr: sdmmc_pwr {
                        rockchip,pins = <0 RK_PA2 RK_FUNC_GPIO &pcfg_output_high>;
                };
        };

	i2c_gpio {
		sx1509_int: sx1509_int{
			rockchip,pins =
			 <0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	lcd_vdd_gpio {
                lcd_vdd_gpio_en: lcd_vdd_gpio_en {
                        rockchip,pins =
                         <3 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>;
                };
        };

	px30_lanxus_gpioset{
		lanxus_gpioset: lanxus-gpioset {
			rockchip,pins = <3 RK_PA6 RK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	secod23_gpio {
		secod23_gpioset: secod23-gpioset {
			rockchip,pins = 
                <0 RK_PA0 RK_FUNC_GPIO &pcfg_output_high>, /* 1P8V_RUN_EN   - GPIO_0  */
                <0 RK_PA1 RK_FUNC_GPIO &pcfg_output_high>, /* M2_SDIO_WAKE# - GPIO_1  */
                <0 RK_PA2 RK_FUNC_GPIO &pcfg_output_high>, /* VCC_AMP_EN    - GPIO_2  */

                <0 RK_PA5 RK_FUNC_GPIO &pcfg_output_high>, /* TOUCH_INT#    - GPIO_5  */

                <0 RK_PB6 RK_FUNC_GPIO &pcfg_output_high>, /* M2_I2C_ALERT# - GPIO_14 */
                <0 RK_PB7 RK_FUNC_GPIO &pcfg_output_high>, /* AMP_SD#       - GPIO_15 */

                <0 RK_PC1 RK_FUNC_GPIO &pcfg_output_high>, /* M2_UART_WAKE# - GPIO_17 */
                <0 RK_PC4 RK_FUNC_1    &pcfg_output_high>, /* CLKOUT_32K  */

                <1 RK_PB0 RK_FUNC_GPIO &pcfg_output_high>, /* IO_EXPB_RST#  - GPIO_40 */
                <1 RK_PB3 RK_FUNC_GPIO &pcfg_output_high>; /* IO_EXPA_RST#  - GPIO_43 */
		};
	};
