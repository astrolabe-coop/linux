	status = "disabled";
};

&route_lvds {
	status = "disabled";
};

&gmac {
	phy-supply = <&vcc_phy>;
	clock_in_out = "input";
    phy-mode = "rmii";
    pinctrl-names = "default";
    pinctrl-0 = <&rmii_pins &mac_refclk_12ma &eth_phy_pwr>;
